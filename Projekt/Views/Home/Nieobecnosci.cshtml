@model IEnumerable<Projekt.Models.Nieobecnosc>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.od)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.@do)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.od)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.@do)
        </td>
        <td>
            @Html.ActionLink("Usuń nieobecność", "UsunNieobecnosc", new { id = item.id })
        </td>
    </tr>
            }
    </table>

    @Html.ActionLink("Dodaj nową nieobecność", "DodajNieobecnosc", "Home")



